{
  "name": "obsidian-better-kanban",
  "version": "0.0.0",
  "description": "Obsidian + Kanban = ❤️",
  "author": "chihiro watanabe",
  "main": "main.js",
  "license": "MIT",
  "private": false,
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "kanban",
    "better kanban",
    "task-management",
    "project-management",
    "productivity",
    "drag-and-drop",
    "react",
    "typescript",
    "markdown"
  ],
  "overrides": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "copy": "test -n \"$OUTDIR\" && mkdir -p \"$OUTDIR\" && cp main.js manifest.json styles.css \"$OUTDIR\" && echo \"✅ Copied to $OUTDIR\" || echo \"ℹ️  Skip copy (set OUTDIR to enable)\"",
    "dev": "npm run lint && rollup -c && npm run copy",
    "build": "NODE_ENV=production npm run lint && rollup -c && npm run copy",
    "test": "jest",
    "test:watch": "yarn test -- --watch"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "lucide-react": "^0.548.0",
    "obsidian": "obsidianmd/obsidian-api#master",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-replace": "2.4.1",
    "@rollup/plugin-typescript": "8.2.0",
    "@types/moment": "2.13.0",
    "@types/node": "14.14.34",
    "@types/papaparse": "5.2.5",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "babel": "6.23.0",
    "eslint": "7.22.0",
    "rollup": "2.41.2",
    "typescript": "^5.7.2"
  }
}
