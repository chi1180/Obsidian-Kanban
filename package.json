{
  "name": "obsidian-kanban-view",
  "version": "1.0.1",
  "description": "Kanban view for Obsidian bases.",
  "author": "chihiro watanabe",
  "main": "main.js",
  "license": "MIT",
  "private": false,
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "kanban",
    "better kanban",
    "task-management",
    "project-management",
    "productivity",
    "drag-and-drop",
    "react",
    "typescript",
    "markdown"
  ],
  "overrides": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "copy": "test -n \"$OUTDIR\" && mkdir -p \"$OUTDIR\" && cp main.js manifest.json styles.css \"$OUTDIR\" && echo \"✅ Copied to $OUTDIR\" || echo \"ℹ️  Skip copy (set OUTDIR to enable)\"",
    "dev": "npm run lint && node esbuild.config.mjs && npm run copy",
    "watch": "node esbuild.config.mjs --watch",
    "build": "NODE_ENV=production npm run lint && node esbuild.config.mjs && npm run copy",
    "test": "jest",
    "test:watch": "yarn test -- --watch"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "lucide-react": "^0.548.0",
    "obsidian": "obsidianmd/obsidian-api#master",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@types/moment": "2.13.0",
    "@types/node": "14.14.34",
    "@types/papaparse": "5.2.5",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "esbuild": "^0.24.2",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.39.1",
    "eslint-plugin-obsidianmd": "^0.1.8",
    "typescript": "^5.7.2"
  }
}
